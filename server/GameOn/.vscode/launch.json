{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [
    {
        "name": ".NET Core Launch (Tournaments)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/GameOn.Tournaments/bin/Debug/netcoreapp3.1/GameOn.Tournaments.dll",
        "args": [],
        "cwd": "${workspaceFolder}/GameOn.Tournaments",
        "stopAtEntry": false,
        "serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
        }
    },
    {
        "name": ".NET Core Launch (Users)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/GameOn.Users/bin/Debug/netcoreapp3.1/GameOn.Users.dll",
        "args": [],
        "cwd": "${workspaceFolder}/GameOn.Users",
        "stopAtEntry": false,
        "serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
        }
    },
    {
        "name": ".NET Core Attach",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:pickProcess}"
    },
    {
        "name": ".NET Core Launch (tournaments) with Dapr",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "daprd-debug-tournaments",
        "program": "${workspaceFolder}/GameOn.Tournaments/bin/Debug/netcoreapp3.1/GameOn.Tournaments.dll",
        "args": [],
        "cwd": "${workspaceFolder}/GameOn.Tournaments",
        "stopAtEntry": false,
        "serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
        },
        "postDebugTask": "daprd-down-tournaments"
    },
    {
        "name": ".NET Core Launch (Users) with Dapr",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "daprd-debug-users",
        "program": "${workspaceFolder}/GameOn.Users/bin/Debug/netcoreapp3.1/GameOn.Users.dll",
        "args": [],
        "cwd": "${workspaceFolder}/GameOn.Users",
        "stopAtEntry": false,
        "serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
        },
        "postDebugTask": "daprd-down-users"
    }
],
    "compounds": [
        {
            "name": "gameon",
            "configurations": [".NET Core Launch (tournaments) with Dapr", ".NET Core Launch (Users) with Dapr"],
            "preLaunchTask": "build"
        }
    ]
}